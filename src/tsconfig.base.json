{
	"compilerOptions": {
		"module": "amd",
		"moduleResolution": "classic",
		"noImplicitAny": false,
		"target": "es5",
		"experimentalDecorators": true,
		"noImplicitReturns": true,
		"noUnusedLocals": false,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"strictBindCallApply": true,
		"baseUrl": ".",
		"paths": {
			"vs/*": [
				"./vs/*"
			],
			"sql/*": [
				"./sql"
			],
			"./sqltest": [
				"./sqltest"
			]
		},
		"types": [
			"keytar",
			"minimist",
			"mocha",
			"semver",
			"sinon",
			"winreg"
		]
	}
}
